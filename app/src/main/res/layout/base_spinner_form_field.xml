<?xml version="1.0" encoding="utf-8"?>
<co.condorlabs.customcomponents.customedittext.CustomTextInputLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/tilBase"
        style="@style/TextFormFieldTheme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <co.condorlabs.customcomponents.customspinner.CustomBaseInstantAutoCompleteTextView
            android:id="@+id/actvBase"
            android:completionThreshold="0"
            android:layout_width="match_parent"
            android:padding="16dp"
            android:drawableEnd="@drawable/ic_down_arrow"
            android:layout_height="wrap_content"/>
</co.condorlabs.customcomponents.customedittext.CustomTextInputLayout>
